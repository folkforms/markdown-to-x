{
  "name": "markdown-to-x",
  "version": "0.0.2",
  "description": "Convert markdown documents to other formats, e.g. automated tests.",
  "main": "cli.js",
  "directories": {
    "doc": "docs"
  },
  "bin": {
    "markdown-to-x": "./cli.js"
  },
  "scripts": {
    "release": "npm whoami && npm version patch && npm publish",
    "example-bte": "node cli.js -- --input=packages/examples/browser-test-example/inputs/bte-doc*.md --structure=packages/examples/browser-test-example/inputs/bte-structure.md --mappings=packages/examples/browser-test-example/inputs/bte-mappings.json --template=packages/examples/browser-test-example/inputs/bte-template.js --output=packages/examples/browser-test-example/outputs",
    "example-ute": "node cli.js -- --input=packages/examples/unit-test-example/inputs/ute-doc*.md --structure=packages/examples/unit-test-example/inputs/ute-structure.md --template=packages/examples/unit-test-example/inputs/ute-template.js --output=packages/examples/unit-test-example/outputs",
    "test": "lerna run test --ignore integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/folkforms/markdown-to-x.git"
  },
  "keywords": [
    "markdown",
    "convert",
    "automated",
    "test",
    "tests"
  ],
  "author": "folkforms",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/folkforms/markdown-to-x/issues"
  },
  "homepage": "https://github.com/folkforms/markdown-to-x#readme",
  "dependencies": {
    "@folkforms/file-io": "^0.2.0",
    "glob": "^7.1.7",
    "jest": "^27.0.4",
    "lerna": "^4.0.0"
  }
}
